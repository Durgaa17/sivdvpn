# ====== OpenTunnel.net - US SIVD - FULL WORKING VERSION ======
client
dev tun
proto tcp

remote 209.141.62.12:80@www.speedtest.net
remote 209.141.62.12:443@www.speedtest.net
remote 209.141.62.12:1194@www.speedtest.net

http-proxy 209.141.62.12 8080
http-proxy-option CUSTOM-HEADER Host www.speedtest.net
http-proxy-option CUSTOM-HEADER X-Online-Host www.speedtest.net
http-proxy-option CUSTOM-HEADER X-Forward-Host www.speedtest.net
http-proxy-option CUSTOM-HEADER Connection: Keep-Alive

auth-user-pass
<auth-user-pass>
opentunnel.net-raam
raam123
</auth-user-pass>

auth-nocache
resolv-retry infinite
route-method exe
cipher AES-128-CBC
nobind
persist-key
persist-tun
remote-cert-tls server
comp-lzo
verb 3

setenv FRIENDLY_NAME "OpenTunnel US SIVD (Full)"

